<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <record model="ir.ui.view" id="res_partner_view_form">
      <field name="name">res.partner.form.import</field>
      <field name="model">res.partner</field>
      <field name="inherit_id" ref="base.view_partner_form"/>
      <field name="type">form</field>
      <field name="arch" type="xml">
        <field name="category_id" position="after">
          <field name="user_channel" />
          <field name="estado" />

         </field>
       </field>
    </record>

    <record model="ir.ui.view" id="res_partner_view_search_import">
      <field name="name">res.partner.form.search.import</field>
      <field name="model">res.partner</field>
      <field name="inherit_id" ref="base.view_res_partner_filter"/>
      <field name="type">search</field>
      <field name="arch" type="xml">
        <filter name="type_company" position="after">
          <filter name="imported" string="Imported" domain="[('imported','=',1)]" />


        </filter>
       </field>
    </record>
  </data>
</odoo>
